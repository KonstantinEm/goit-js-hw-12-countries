{{#each this}}
<h2 class="item-name">{{name}}</h2>
<div class="country-item">
  <div class="item-content">
    <p class="item-capital">Capital: {{capital}}</p>
    <p class="item-population">Population: {{population}}</p>
    <p class="lang-list__item">Languages: </p>
    <ul>
       {{#each languages}}
       <li>{{name}}</li>
       {{/each}}
    </ul>
  </div>
    <img
    src="{{flag}}"
    alt="{{nativeName}}"
    class="item-image"
  />
</div>
{{/each}}